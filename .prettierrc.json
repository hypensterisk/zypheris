{
  "arrowParens": "always",
  "bracketSameLine": true,
  "bracketSpacing": true,
  "endOfLine": "lf",
  "jsxSingleQuote": true,
  "plugins": ["prettier-plugin-sort-json", "prettier-plugin-packagejson"],
  "printWidth": 80,
